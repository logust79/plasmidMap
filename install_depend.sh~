#!/bin/sh

# this is to install dependent programs/perl modules

# perl version >= 14?

ver=$(perl -v | awk '/version [0-9]+/ {print $6}' | awk '{print substr($0,0,2)}')

if [ $ver -ge 24 ] ; then
	echo 'yeah';
else
	echo 'Error: Your perl version has to be at least 5.14.0. perl -v to check your perl version';
	exit 1;
fi
